/sei/deliverables/gift_tracker/client/src/pages/Recipient.js",["141"],"import React, { Fragment, useEffect } from 'react';\nimport PropTypes from 'prop-types';\nimport { connect } from 'react-redux';\nimport Spinner from '../layout/Spinner';\nimport { getRecipient } from '../actions/recipient';\nimport RecipientCard from '../components/RecipientCard';\nimport GiftCard from '../components/GiftCard';\n\nconst Recipient = ({ getRecipient, recipient: { recipient, loading}, match }) => {\n\n  useEffect(() => {\n    getRecipient(match.params.id);\n  }, [getRecipient]);\n\n  return loading || recipient === null ? <Spinner /> : <Fragment>\n    <RecipientCard recipient={recipient} recipientId={recipient._id} />\n    {recipient.gifts.map(gift => {\n      return <GiftCard key={gift._id} gift={gift} recipientId={recipient._id}/>\n    })}\n  </Fragment>\n}\n\nRecipient.propTypes = {\n  getRecipient: PropTypes.func.isRequired,\n  recipient: PropTypes.object.isRequired\n};\n\nconst mapStateToProps = state => ({\n  recipient: state.recipient\n});\n\nexport default connect(mapStateToProps, {getRecipient}) (Recipient);","/Users/willvanhook/sei/deliverables/gift_tracker/client/src/reducers/alert.js",[],["142","143"],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/actions/types.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/components/Alert.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/actions/alert.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/reducers/auth.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/actions/auth.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/utils/setAuthToken.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/pages/Main.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/routing/PrivateRoute.js",[],"/Users/willvanhook/sei/deliverables/gift_tracker/client/src/reducers/recipient.js",[],"/Users/willvanhook/sei/deliv